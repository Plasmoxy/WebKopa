{
  "compilerOptions": {
    "module": "none",
    "checkJs": false,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "noImplicitAny": true,
    "outFile": "./build/build.js",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "ESNext",
  }
}