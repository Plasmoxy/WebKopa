{
  "compilerOptions": {
      "target": "esnext",
      "module": "es6",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "jsx": "react",
      "lib": [
          "dom",
          "dom.iterable",
          "es6",
      ],
      "allowJs": true,
      "experimentalDecorators": true,
      "noImplicitAny": true,
      "strict": true,
      "sourceMap": true,
      "resolveJsonModule": true,
      "noEmit": true,
      "rootDir": "./src",
  }
}